  "private": true,
  "scripts": {
    "prebuild": "rimraf public/static/build",
    "build": "set NODE_ENV=production&&webpack -p --progress",
    "git": "git add --all",
    "postgit": "git commit -m \"heroku deploy\"",
    "predeploy": "npm run build",
    "deploy": "npm run git",
    "postdeploy": "git push heroku master",
    "dev": "set NODE_ENV=development&&npm-run-all --parallel devp nodemon",
    "prestart2": "npm run build",
    "start": "NODE_ENV=production node server/runner.js",
    "r": "set NODE_ENV=production&&node server/runner.js",
    "nodemon": "nodemon --exec babel-node -- server/runner.js",
    "devp": "webpack-dev-server --hot --debug --devtool source-map --output-pathinfo --watch --colors --progress  --inline --content-base public --port 8050 --host 0.0.0.0",
    "extract_lang": "./bin/extract_lang.sh",
    "lint": "eslint shared client server",
    "test": "npm run lint",
    "po2json": "./bin/po2json.sh",
    "stop": "set NODE_ENV='production'&&pm2 delete real-estate-react-relay -s &> /dev/null"
  },
